{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Storage": {
    "QueuesFolder": "../../artifacts/queues",
    "BlobFolder": "../../artifacts/fileStorage"
  },
  "Database": {
    "Path": "Filename=../../artifacts/oliver.db; Connection=Shared;"
  },
  "LogFile": {
    "FileName": "../../artifacts/logs/server.log", // sln folder
    "ArchiveLogFileFormat": "../../artifacts/logs/server.{0}.log",
    "MaxLogSizeMb": 1,
    "MaxFilesCount": 3
  }
}
