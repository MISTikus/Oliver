{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Server": {
    "BaseUrl": "https://localhost:5001/"
  },
  "Instance": {
    "Tenant": "some",
    "Environment": "dev"
  },
  "LogFile": {
    "FileName": "./logs/log.log",
    "ArchiveLogFileFormat": "./logs/log.{0}.log",
    "MaxLogSizeMb": 1,
    "MaxFilesCount": 3
  }
}
