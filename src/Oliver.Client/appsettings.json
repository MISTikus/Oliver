{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Server": {
    "BaseUrl": "https://localhost:5001/"
  },
  "Client": {
    "DefaultFolder": "../../../../artifacts/workFolder", // sln folder
    "Instances": [
      {
        "Tenant": "Some",
        "Environment": "Prod"
      }
    ]
  },
  "LogFile": {
    "FileName": "../../../../artifacts/logs/client.log", // sln folder
    "ArchiveLogFileFormat": "../../../../artifacts/logs/client.{0}.log",
    "MaxLogSizeMb": 1,
    "MaxFilesCount": 3
  }
}
