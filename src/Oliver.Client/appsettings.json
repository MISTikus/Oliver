{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Server": {
    "BaseUrl": "https://localhost:5001/"
  },
  "Client": {
    "DefaultFolder": "../../../../workFolder", // sln folder
    "Instances": [
      {
        "Tenant": "Some",
        "Environment": "Prod"
      }
    ]
  },
    "LogFile": {
      "FileName": "../../../../logs/log.log", // sln folder
      "ArchiveLogFileFormat": "./logs/log.{0}.log",
      "MaxLogSizeMb": 1,
      "MaxFilesCount": 3
    }
  }
